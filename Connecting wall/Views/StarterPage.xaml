<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Connecting_wall.Views.StarterPage"
             Title="StarterPage">
    <VerticalStackLayout>
        <Label Text="Enter Team 1 Name"/>
        <Entry x:Name="TeamOneEnt"/>
        <Label Text="Enter Team 2 Name" />
        <Entry x:Name="TeamTwoEnt"/>
        <Button Text="Start Game" Clicked="GoToRoundOne"/>
    </VerticalStackLayout>
</ContentPage>